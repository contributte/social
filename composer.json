{
  "name": "contributte/social",
  "description": "Social plugins for Nette Framework",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/social",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">= 7.1",
    "nette/application": "~2.4.12",
    "nette/utils": "~2.5.2"
  },
  "require-dev": {
    "ninjify/qa": "^0.8.0",
    "ninjify/nunjuck": "^0.2.0",
    "phpstan/phpstan": "^0.12.2",
    "phpstan/phpstan-deprecation-rules": "^0.12.0",
    "phpstan/phpstan-nette": "^0.12.0",
    "phpstan/phpstan-strict-rules": "^0.12.0"
  },
  "conflict": {
    "nette/http": "<=2.4.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Social\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  }
}
