{
  "name": "contributte/social",
  "description": "Social plugins for Nette Framework",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/social",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.1",
    "nette/application": "^3.0.0",
    "nette/utils": "^3.0.0"
  },
  "require-dev": {
    "ninjify/qa": "^0.13",
    "ninjify/nunjuck": "^0.4",
    "phpstan/phpstan": "^1.0.0",
    "phpstan/phpstan-deprecation-rules": "^1.0.0",
    "phpstan/phpstan-nette": "^1.0.0",
    "phpstan/phpstan-strict-rules": "^1.0.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Social\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "2.1.x-dev"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
